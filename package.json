{
  "name": "notification-widget",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": "eslint --fix --quiet"
  },
  "scripts": {
    "prepare": "husky install",
    "build:noEmit": "yarn tsc --noEmit",
    "clean-dist": "rimraf dist",
    "build-package": "npm run clean-dist && node esbuild.config.ts && tsc --emitDeclarationOnly --outFile dist/index.js",
    "start": "node esbuild-serve.js -w",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@emotion-icons/octicons": "^3.27.0",
    "@epnsproject/frontend-sdk-staging": "^1.8.5",
    "@epnsproject/sdk-restapi": "^0.1.13",
    "dayjs": "^1.11.5",
    "ethers": "^5.6.9",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tiny-popover": "^7.1.0",
    "react-verification-input": "^3.1.0",
    "siwe": "^1.1.6",
    "styled-components": "^5.3.5",
    "wagmi": "^0.6.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/near-operation-file-preset": "^2.4.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "esbuild": "^0.15.5",
    "esbuild-register": "^3.3.3",
    "esbuild-serve": "^1.0.1",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.2.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist"
  ]
}
